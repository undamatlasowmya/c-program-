//program to find a given number is prime or not
#include<stdio.h>
#include<conio.h>
main()
{
	int n,i,c=0;
	printf("Enter the given number\n");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
	if(n%i==0)
{
	c++;
}
	}
	if(c==2)
	{
		printf("prime number: %d",n);
	}
	else
	{
		printf("non prime number : %d\n",n);
	}
}
