//program to print second highest among the numbers
/*eg:
input:
1,23,4,5,6
output:
6
6 is the 2nd highest among the numbers*/
#include<stdio.h>
#include<conio.h>
main()
{
	int n,i=1,h,ele;  //ele means element
	printf("Enter the no.of numbers\n");
	scanf("%d",&n);
	while(i<=n)
	{
		scanf("%d",&ele);
		if(i==1)
		h=ele;
		if(ele>=h&&i<=n)
		h=ele;
		i++;
	}
	    if(ele=h)
		printf( " second largest number: %d\n",ele);
}
